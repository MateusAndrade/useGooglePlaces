{
  "name": "use-google-places",
  "version": "1.0.1",
  "description": "React hook that consumes Google Places API returning both autocomplete and geocoding callback method",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrobullo/use-google-places"
  },
  "keywords": [
    "react",
    "react native",
    "autocomplete",
    "google api",
    "google places",
    "geocoding"
  ],
  "author": "Pedro Bullo <pedro.bj@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.9",
    "@types/react": "^16.9.46",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "jest": "26.3.0",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "react": "16.13.1"
  },
  "files": [
    "lib/**/*"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
